{
	"name": "frontend",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@eslint/compat": "catalog:sveltekit-dev",
		"@eslint/js": "catalog:sveltekit-dev",
		"@inlang/paraglide-js": "catalog:sveltekit-dev",
		"@sveltejs/adapter-auto": "catalog:sveltekit-dev",
		"@sveltejs/kit": "catalog:sveltekit-dev",
		"@sveltejs/vite-plugin-svelte": "catalog:sveltekit-dev",
		"@tailwindcss/vite": "catalog:sveltekit-dev",
		"@types/node": "catalog:sveltekit-dev",
		"eslint": "catalog:sveltekit-dev",
		"eslint-config-prettier": "catalog:sveltekit-dev",
		"eslint-plugin-svelte": "catalog:sveltekit-dev",
		"globals": "catalog:sveltekit-dev",
		"prettier": "catalog:sveltekit-dev",
		"prettier-plugin-svelte": "catalog:sveltekit-dev",
		"prettier-plugin-tailwindcss": "catalog:sveltekit-dev",
		"svelte": "catalog:sveltekit-dev",
		"svelte-check": "catalog:sveltekit-dev",
		"tailwindcss": "catalog:sveltekit-dev",
		"typescript": "catalog:sveltekit-dev",
		"typescript-eslint": "catalog:sveltekit-dev",
		"vite": "catalog:sveltekit-dev"
	}
}
